// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model premium_users {
  user_id Int @id
} 

model premium_post {
  post_id Int @id
}

model premium_admin {
  admin_id Int @id @default(autoincrement())
  email String
  fullname String
  username String
  password String
  profile_path String
}

